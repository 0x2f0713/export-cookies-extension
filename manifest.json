{
  "manifest_version": 2,
  "name": "Export cookies",
  "version": "0.0.1",
  "description": "Export cookies for specific website",
  "icons": {},
  "permissions": ["tabs", "cookies", "http://*/*", "https://*/*", "storage"],
  "browser_action": {},
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'  blob: filesystem: chrome-extension-resource:; object-src 'self'"
}
